#!/usr/bin/env groovy 

import NeedlemanWunsch

String seq1 = "";
String seq2 = "";


int misMatchPenalty = 3;
int gapPenalty = 2;

n = args[0] as int
println "Doing ${n} alignments:"

for(int i = 0; i < n;i++){
	nw = new NeedlemanWunsch(seq1,seq2,misMatchPenalty,gapPenalty)
	nw.findMinPenalty();
}

